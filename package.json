  },
  "main": "app.js",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "connect-mongodb-session": "^2.0.6",
    "cookie-parser": "^1.4.3",
    "esptool-wrapper": "^1.0.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mongoose": "^5.3.8",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
